{
  "name": "react-app-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/arnavyash/react-app-boilerplate.git",
  "author": "Arnav Yash <arnavyash2004@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start:dev": "webpack-dev-server --env.mode development",
    "build": "rimraf dist && webpack --env.mode production",
    "build:dev": "rimraf dist && webpack --env.mode development",
    "lint": "eslint --format=pretty --cache --ext js,jsx .",
    "lint:fix": "npm run lint -- --fix",
    "fmt": "cross-env prettier --write '**/{.[^.],}*.{html,md,mdx,yml,yaml,json,css,scss,less}'",
    "fmt:check": "cross-env prettier-check '**/{.[^.],}*.{html,md,mdx,yml,yaml,json,css,scss,less}'"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.0.5",
    "cross-env": "5.2.0",
    "eslint": "5.13.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-formatter-pretty": "2.1.1",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "friendly-errors-webpack-plugin": "1.7.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "prettier": "1.16.4",
    "prettier-check": "2.0.0",
    "rimraf": "2.6.3",
    "webpack": "4.29.3",
    "webpack-bundle-analyzer": "3.0.4",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "4.2.1",
    "webpackbar": "3.1.5"
  },
  "dependencies": {
    "app-root-path": "2.1.0",
    "react": "16.8.1",
    "react-dom": "16.8.1"
  }
}
